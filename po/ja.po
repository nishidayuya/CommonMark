"- baz\n"
"`````````````````````````````````````````````````````````\n"
msgstr ""

#. type: Plain text
#: spec.md:4987
#, no-wrap
msgid ""
"````````````````````````````````````````````````````````` html\n"
"<ul>\n"
"<li>foo</li>\n"
"</ul>\n"
"<p>bar</p>\n"
"<ul>\n"
"<li>baz</li>\n"
"</ul>\n"
"`````````````````````````````````````````````````````````\n"
msgstr ""

#. type: Plain text
#: spec.md:4992
msgid "Indeed, two blank lines will end *all* containing lists:"
msgstr ""

#. type: Plain text
#: spec.md:4996
#, no-wrap
msgid ""
