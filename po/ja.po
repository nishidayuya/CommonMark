msgstr ""

#. type: Plain text
#: spec.md:4969
#, no-wrap
msgid ""
"<div class=\"example\" id=\"example-189\" data-section=\"Lists\">\n"
"<div class=\"examplenum\">Example 189</div>\n"
msgstr ""

#. type: Plain text
#: spec.md:4977
#, no-wrap
msgid ""
