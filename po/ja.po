"<li>foo</li>\n"
"<li>bar</li>\n"
"</ol>\n"
"<ol start=\"3\">\n"
"<li>baz</li>\n"
"</ol>\n"
"`````````````````````````````````````````````````````````\n"
msgstr ""

#. type: Plain text
#: spec.md:4935
msgid "There can be blank lines between items, but two blank lines end a list:"
msgstr ""

#. type: Plain text
#: spec.md:4939
#, no-wrap
msgid ""
"<div class=\"example\" id=\"example-188\" data-section=\"Lists\">\n"
"<div class=\"examplenum\">Example 188</div>\n"
msgstr ""

#. type: Plain text
#: spec.md:4944
msgid "- bar"
msgstr ""

#. type: Plain text
#: spec.md:4948
msgid "- baz `````````````````````````````````````````````````````````"
msgstr ""

#. type: Plain text
#: spec.md:4958
#, no-wrap
msgid ""
"````````````````````````````````````````````````````````` html\n"
